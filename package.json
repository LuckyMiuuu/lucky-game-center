{
  "name": "vue3-vite-mobile",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "prod": "vite --mode prod",
    "test": "vite --mode test",
    "build:dev": "run-p type-check && vite build --mode dev",
    "build:prod": "run-p type-check && vite build --mode prod",
    "build:test": "run-p type-check && vite build --mode test",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@ton/core": "^0.56.3",
    "@tonconnect/ui": "^2.0.8",
    "@types/crypto-js": "^4.2.2",
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "buffer": "^6.0.3",
    "clipboard": "^2.0.11",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "decimal.js": "^10.4.3",
    "nanoid": "^5.0.7",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "postcss-loader": "^8.1.1",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.77.4",
    "vant": "^4.9.2",
    "vue": "^3.4.21",
    "vue-mobile-detection": "^2.0.1",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.5",
    "@types/postcss-pxtorem": "^6.0.3",
    "@vant/auto-import-resolver": "^1.2.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.1.2",
    "postcss-pxtorem": "^6.1.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.18.0",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.2.8",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^2.0.11"
  }
}
